{
  "metadata": {
    "name": "feckinhaproxy"
  },
  "kind": "DaemonSet",
  "spec": {
    "template": {
      "metadata": {
        "labels": {
          "app": "feckinhaproxy"
        }
      },
      "spec": {
        "nodeSelector": {
          "kubernetes.io/hostname": "192.168.66.2"
        },
        "containers": [
          {
            "imagePullPolicy": "Always",
            "image": "fiveateooate/feckinhaproxy:1234",
            "name": "feckinhaproxy",
            "ports": [
              {
                "containerPort": 80
              }
            ]
          }
        ]
      }
    }
  },
  "apiVersion": "extensions/v1beta1"
}
